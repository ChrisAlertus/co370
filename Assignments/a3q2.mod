/********************************************* * OPL 12.4 Model * Author: cvalert * Creation Date: Feb 1, 2014 at 10:30:42 PM *********************************************/int numprod = ...; // number of products we makerange Products = 1..numprod; // indices for each productfloat Profit[Products] = ...;  // profit for each productfloat Carpentry[Products] = ...; // carpentry required per productfloat Finishing[Products] = ...; // finishing required per productfloat Wood[Products] = ...; // wood board required per productfloat MaxCarp = ...; // available carpentry hoursfloat MaxFinish = ...; // available finshing hourfloat MaxWood = ...; // available wood boardsdvar float+ Amount[Products]; // amounts of each product producedmaximize  sum( i in Products ) Profit[i] * Amount[i]; subject to {  sum ( j in Products ) Carpentry[j] * Amount[j] <= MaxCarp; // use at most available carpentry   sum ( j in Products ) Finishing[j] * Amount[j] <= MaxFinish; // use at most available finishing  sum ( j in Products ) Wood[j] * Amount[j] <= MaxWood; // use at most available wood}execute {      IloOplImportJava("SA.jar");      write(IloOplCallJava("SA", "sa", "", thisOplModel));    }